	<!-- ==========================================
       SYSTEM.WEB  
       ========================================== -->
	<system.web>
		<!-- FIX: Broken Tag Corrected -->
		<httpCookies requireSSL="false" httpOnlyCookies="true"/>
		<!-- FIX: Required for CAPTCHA & External Session -->
		<sessionState mode="InProc" timeout="40" cookieless="UseCookies"/>
		<!-- FIX: MUST HAVE FOR CAPTCHA TO WORK OUTSIDE -->
		<machineKey validationKey="8F7C9D0A6E2B4F1D3C78A5B0F6E9D2C1A4B7C8D9E2F3A5C6B7D8E9F0A1B2C3D" decryptionKey="A1B2C3D4E5F60718293A4B5C6D7E8F90" validation="SHA1" decryption="AES"/>
		<customErrors mode="Off"/>
		<pages enableViewStateMac="true" viewStateEncryptionMode="Always"/>
		<compilation targetFramework="4.6" debug="true"/>
		<httpRuntime targetFramework="4.6" maxRequestLength="2097152" executionTimeout="3600" enableVersionHeader="false"/>
	</system.web>
	<!-- ==========================================
       SYSTEM.WEBSERVER
       ========================================== -->
	<system.webServer>
		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="2147483645"/>
				<verbs allowUnlisted="false">
					<add verb="GET" allowed="true"/>
					<add verb="POST" allowed="true"/>
					<add verb="HEAD" allowed="false"/>
					<add verb="PUT" allowed="false"/>
					<add verb="DELETE" allowed="false"/>
					<add verb="OPTIONS" allowed="false"/>
					<add verb="TRACE" allowed="false"/>
				</verbs>
			</requestFiltering>
		</security>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By"/>
				<add name="x-aspnetmvc-version" value="none"/>
				<add name="X-XSS-Protection" value="1; mode=block"/>
				<add name="X-Content-Type-Options" value="nosniff"/>
				<add name="Referrer-Policy" value="no-referrer"/>
				<add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains;preload"/>
				<add name="Permission-Policy" value="geolocation=(),microphone=(),camera=()"/>
				<add name="Content-Security-Policy" value="none"/>
				<!--<add name="Cache-Control" value="no-cache, no-store, must-revalidate"/>-->
				<add name="Cache-Control" value="no-store"/>
				<add name="Pragma" value="no-cache"/>
				<add name="Expires" value="1"/>
			</customHeaders>
		</httpProtocol>
		<!--<httpErrors errorMode="Custom" existingResponse="Replace">
      <remove statusCode="404"/>
